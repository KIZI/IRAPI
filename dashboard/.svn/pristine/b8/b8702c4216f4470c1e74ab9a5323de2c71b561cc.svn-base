package cz.janbouchner.ir.dashboard.crate;

import cz.janbouchner.ir.dashboard.domain.Domain;

public class WatchedItem {
	String domain;
	String context;
	int count;

    public WatchedItem(String domain, String context) {
		super();
		this.domain = domain;
		this.context = context;
		this.count = 0;        
    }

	public int getCount() {
		return count;
	}

	public void setCount(int count) {
		this.count = count;
	}

	public String getDomain() {
		return domain;
	}

	public String getContext() {
		return context;
	}

	@Override
	public String toString() {
		return domain + ":" + context + ":" + count;
	}
}